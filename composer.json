{
  "name": "interview",
  "description": "AppJobs interview exercise",
  "keywords": ["jobs", "offers"],
  "license": "MIT",
  "type": "project",
  "prefer-stable": true,
  "require": {
    "php": ">=7.1.3"
  },
  "require-dev": {
    "mockery/mockery": "1.3.0",
    "phpunit/phpunit": "~7.0"
  },
  "autoload": {
    "psr-4": {
      "AppJobs\\": "src/"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "Tests\\UnitTest\\": "tests/unittest"
    }
  },
  "scripts": {
    "test": "vendor/bin/phpunit"
  },
  "config": {
    "preferred-install": "dist"
  }
}
